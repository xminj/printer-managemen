<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.ComboBox?>

<VBox xmlns:fx="http://javafx.com/fxml" alignment="CENTER" spacing="10"
      fx:controller="com.xminj.printermanagemen.controller.PrinterManagementController">

    <!-- ComboBox 用于显示打印机列表  -->
    <ComboBox fx:id="printerComboBox" prefWidth="250"/>

    <!-- Button 触发打印 -->
    <Button text="Print" onAction="#handlePrintAction" prefWidth="250"/>
</VBox>